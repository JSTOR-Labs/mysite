  },
  {
    "type": "Annotation",
    "body": [
      {
        "type": "TextualBody",
        "value": "This is Nahua codex created in the early period of Spanish colonization. Here, the patient (right) sits in front of a divine healer (left). ",
        "purpose": "commenting"
      }
    ],
    "target": {
      "source": "https://free.iiifhosting.com/iiif/19f2c7680ef41a22e29bbe06f09b76bb4b142f8713f6d082dc78e1c8fdf5cf2d",
      "selector": {
